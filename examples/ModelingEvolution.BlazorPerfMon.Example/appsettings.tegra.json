{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MonitorSettings": {
    "NetworkInterface": "eth0",
    "DiskDevice": "mmcblk0",
    "CollectionIntervalMs": 500,
    "DataPointsToKeep": 120,
    "GpuCollectorType": "NvTegra",
    "Layout": [
      [ "CPU/8", "GPU/1", "Docker", "ComputeLoad/3|col-span:2" ],
      [ "Network:eth0/3", "Disk:mmcblk0/3" ]
    ]
  }
}
