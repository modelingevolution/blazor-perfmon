{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MonitorSettings": {
    "NetworkInterface": "eth0,eth1",
    "DiskDevice": "sdd",
    "CollectionIntervalMs": 500,
    "DataPointsToKeep": 120,
    "GpuCollectorType": "Nvml",
    "Layout": [
      [ "CPU/16", "GPU/1", "Docker", "ComputeLoad/3|col-span:2" ],
      [ "Network:eth0/2", "Network:eth1/2", "Disk:sdd/2" ]
    ]
  }
}
